{
  "MD013": {
    "line_length": 256,
    "heading_line_length": 120,
    "code_block_line_length": 200,
    "code_blocks": false,
    "tables": true,
    "headings": true,
    "strict": false
  },
  "MD003": {
    "style": "atx"
  },
  "MD004": {
    "style": "dash"
  },
  "MD007": {
    "indent": 2
  },
  "MD009": {
    "br_spaces": 2,
    "list_item_empty_lines": false,
    "strict": false
  },
  "MD012": {
    "maximum": 2
  },
  "MD026": {
    "punctuation": ".,;:!?"
  },
  "MD029": {
    "style": "one_or_ordered"
  },
  "MD033": {
    "allowed_elements": ["br", "details", "summary", "sup", "sub"]
  },
  "MD035": {
    "style": "---"
  },
  "MD046": {
    "style": "fenced"
  },
  "MD048": {
    "style": "backtick"
  },
  "MD049": {
    "style": "asterisk"
  },
  "MD050": {
    "style": "underscore"
  }
}
